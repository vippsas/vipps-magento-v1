<layout>
    <adminhtml_vipps_quotemonitoring_index>
        <reference name="content">
            <block
                    type="vipps_payment/adminhtml_quoteMonitoring_container"
                    name="quotemonitoring_widget_container"/>
        </reference>
    </adminhtml_vipps_quotemonitoring_index>

    <adminhtml_vipps_quotemonitoring_view>
        <reference name="content">
            <block type="vipps_payment/adminhtml_quoteMonitoring_buttons"
                   name="quotemonitoring_buttons"
                   template="vippspayment/monitoring/buttons.phtml"/>
            <block type="vipps_payment/adminhtml_quoteMonitoring_view"
                   name="quotemonitoring_view"
                   template="vippspayment/monitoring/view.phtml"/>
        </reference>
    </adminhtml_vipps_quotemonitoring_view>

    <adminhtml_vipps_requestprofiling_index>
        <reference name="content">
            <block type="vipps_payment/adminhtml_requestProfiling_container"
                   name="requestprofiling_widget_container"/>
        </reference>
    </adminhtml_vipps_requestprofiling_index>

    <adminhtml_vipps_requestprofiling_view>
        <reference name="content">
            <block type="adminhtml/template"
                   name="requestprofiling_buttons"
                   template="vippspayment/profiling/buttons.phtml"/>
            <block type="vipps_payment/adminhtml_requestProfiling_view"
                   name="requestprofiling_view"
                   template="vippspayment/profiling/view.phtml"/>
        </reference>
    </adminhtml_vipps_requestprofiling_view>
